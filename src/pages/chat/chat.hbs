      {{{profileLink}}}
      <div class="chat__list-search">
        {{{inputSearch}}}
      </div>
    </div>
    <div class="chat__list-cards">
      {{{chatCard}}}
    </div>
  </div>
  <div class="chat__body">
    <div class="chat__body-head">
      {{{chatHeaderCard}}}
    </div>
    <div class="chat__messages">

    </div>
    <div class="chat__message">
      {{{inputMessage}}}
    </div>
  </div>
</div>
